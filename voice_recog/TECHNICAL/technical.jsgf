#JSGF V1.0;
grammar TECHNICAL;
public <technical> = <introduce> | <action> | <command>;

<noise> = PLEASE | ROBOT ;
<introduce> = [<noise>] INTRODUCE YOURSELF [<noise>];
<action> = [<noise>] CRACK [<article>] EGG [<noise>];
<article> = A | AN | THE;
<command> = STOP | ROBOT YES | ROBOT NO | GET OUT | HALTING | HALT | YES | NO ;


