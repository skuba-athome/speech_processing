<launch>

  <node name="recognizer" pkg="pocketsphinx" type="recognizer.py" output="screen">
    <remap from="recognizer/output" to="voice/output" />
    <param name="lm" value="$(find pocketsphinx)/dic/RIPS.lm"/>
    <param name="dict" value="$(find pocketsphinx)/dic/RIPS.dic"/>
  </node>

</launch>
