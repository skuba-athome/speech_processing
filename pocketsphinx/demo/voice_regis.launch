<launch>

  <node name="recognizer" pkg="pocketsphinx" type="recognizer_regis.py">
    <param name="lm" value="$(find pocketsphinx)/demo/voice_regis.lm"/>
    <param name="dict" value="$(find pocketsphinx)/demo/voice_regis.dic"/>
  </node>

  <node name="voice_regis" pkg="pocketsphinx" type="voice_regis.py">
    <param name="xA" type="double" value="41"/>
    <param name="xA" type="double" value="41"/>
    <param name="yA" type="double" value="58.5"/>
    <param name="thB" type="double" value="1.57"/>
    <param name="yB" type="double" value="58.5"/>
    <param name="thB" type="double" value="1.57"/>
  </node>


</launch>
